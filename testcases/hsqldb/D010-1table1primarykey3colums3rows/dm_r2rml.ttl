@prefix rr:      <http://www.w3.org/ns/r2rml#> .
@prefix data:    <http://example.com/base/data/> .
@prefix mapping:  <http://example.com/base/mapping/> .
@prefix vocab:   <http://example.com/base/vocab/> .

<http://example.com/base/mapping/Country%20Info>
      rr:logicalTable
              [ rr:tableName "\"Country Info\""
              ] ;
      rr:subjectMap
              [ rr:class <http://example.com/base/vocab/Country%20Info> ;
                rr:template "http://example.com/base/data/Country%20Info/Country%20Code={\"Country Code\"}"
              ] .
