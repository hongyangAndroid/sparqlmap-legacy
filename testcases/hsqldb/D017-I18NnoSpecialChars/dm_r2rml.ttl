@prefix rr:      <http://www.w3.org/ns/r2rml#> .
@prefix data:    <http://example.com/base/data/> .
@prefix mapping:  <http://example.com/base/mapping/> .
@prefix vocab:   <http://example.com/base/vocab/> .

<http://example.com/base/mapping/%E6%A4%8D%E7%89%A9>
      rr:logicalTable
              [ rr:tableName "\"植物\""
              ] ;
      rr:subjectMap
              [ rr:class <http://example.com/base/vocab/%E6%A4%8D%E7%89%A9> ;
                rr:template "http://example.com/base/data/%E5%90%8D={\"名\"};%E4%BD%BF%E7%94%A8%E9%83%A8={\"使用部\"}"
              ] .

<http://example.com/base/mapping/%E6%88%90%E5%88%86>
      rr:logicalTable
              [ rr:tableName "\"成分\""
              ] ;
      rr:predicateObjectMap
              [ rr:objectMap
                        [ rr:joinCondition
                                  [ rr:child "\"植物名\"" ;
                                    rr:parent "\"名\""
                                  ] ;
                          rr:joinCondition
                                  [ rr:child "\"使用部\"" ;
                                    rr:parent "\"使用部\""
                                  ] ;
                          rr:parentTriplesMap <http://example.com/base/mapping/%E6%A4%8D%E7%89%A9>
                        ] ;
                rr:predicate <http://example.com/base/vocab/%E6%88%90%E5%88%86#%E6%A4%8D%E7%89%A9%E5%90%8D;%E4%BD%BF%E7%94%A8%E9%83%A8>
              ] ;
      rr:subjectMap
              [ rr:class <http://example.com/base/vocab/%E6%88%90%E5%88%86> ;
                rr:template "http://example.com/base/data/" ;
                rr:termType rr:BlankNode
              ] .
