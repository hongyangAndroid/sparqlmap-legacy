@prefix rr:      <http://www.w3.org/ns/r2rml#> .
@prefix data:    <http://example.com/base/data/> .
@prefix mapping:  <http://example.com/base/mapping/> .
@prefix vocab:   <http://example.com/base/vocab/> .

mapping:Country
      rr:logicalTable
              [ rr:tableName "\"Country\""
              ] ;
      rr:subjectMap
              [ rr:class vocab:Country ;
                rr:template "http://example.com/base/data/Code={\"Code\"};Lan={\"Lan\"}"
              ] .
