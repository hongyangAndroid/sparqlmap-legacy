              [ rr:objectMap
                        [ rr:column "\"likedObj\""
                        ] ;
                rr:predicate <http://example.com/base/LIKES#likedObj>
              ] ;
      rr:predicateObjectMap
              [ rr:objectMap
                        [ rr:column "\"likeType\""
                        ] ;
                rr:predicate <http://example.com/base/LIKES#likeType>
              ] ;
      rr:predicateObjectMap
              [ rr:objectMap
                        [ rr:column "\"id\""
                        ] ;
                rr:predicate <http://example.com/base/LIKES#id>
              ] ;
      rr:subjectMap
