@prefix rr:      <http://www.w3.org/ns/r2rml#> .
@prefix data:    <http://example.com/base/> .
@prefix mapping:  <http://example.com/base/> .
@prefix vocab:   <http://example.com/base/> .

vocab:TriplesMap2
      rr:logicalTable
              [ rr:tableName "\"Student\""
              ] ;
      rr:predicateObjectMap
              [ rr:objectMap
                        [ rr:column "\"FirstName\""
                        ] ;
                rr:predicate <http://example.com/base/Student#FirstName>
              ] ;
      rr:predicateObjectMap
              [ rr:objectMap
                        [ rr:column "\"ID\""
                        ] ;
                rr:predicate <http://example.com/base/Student#ID>
              ] ;
      rr:predicateObjectMap
              [ rr:objectMap
                        [ rr:column "\"LastName\""
                        ] ;
                rr:predicate <http://example.com/base/Student#LastName>
              ] ;
      rr:subjectMap
              [ rr:class vocab:Student ;
                rr:template "http://example.com/base/Student/ID={\"ID\"}"
              ] .

vocab:TriplesMap3
      rr:logicalTable
              [ rr:tableName "\"Student_Sport\""
              ] ;
      rr:predicateObjectMap
              [ rr:objectMap
                        [ rr:column "\"ID_Student\""
                        ] ;
                rr:predicate <http://example.com/base/Student_Sport#ID_Student>
              ] ;
      rr:predicateObjectMap
              [ rr:objectMap
                        [ rr:column "\"ID_Sport\""
                        ] ;
                rr:predicate <http://example.com/base/Student_Sport#ID_Sport>
              ] ;
      rr:subjectMap
              [ rr:class vocab:Student_Sport ;
                rr:template "http://example.com/base/ID_Student={\"ID_Student\"};ID_Sport={\"ID_Sport\"}"
              ] .

vocab:TriplesMap1
      rr:logicalTable
              [ rr:tableName "\"Sport\""
              ] ;
      rr:predicateObjectMap
              [ rr:objectMap
                        [ rr:joinCondition
                                  [ rr:child "\"ID\"" ;
                                    rr:parent "\"ID_Sport\""
                                  ] ;
                          rr:parentTriplesMap vocab:TriplesMap1
                        ] ;
                rr:predicate <http://example.com/base/Sport#ID_Sport>
              ] ;
      rr:predicateObjectMap
              [ rr:objectMap
                        [ rr:joinCondition
                                  [ rr:child "\"ID\"" ;
                                    rr:parent "\"ID_Student\""
                                  ] ;
                          rr:parentTriplesMap vocab:TriplesMap2
                        ] ;
                rr:predicate <http://example.com/base/Sport#ID_Student>
              ] ;
      rr:predicateObjectMap
              [ rr:objectMap
                        [ rr:column "\"Description\""
                        ] ;
                rr:predicate <http://example.com/base/Sport#Description>
              ] ;
      rr:predicateObjectMap
              [ rr:objectMap
                        [ rr:column "\"ID\""
                        ] ;
                rr:predicate <http://example.com/base/Sport#ID>
              ] ;
      rr:subjectMap
              [ rr:class vocab:Sport ;
                rr:template "http://example.com/base/Sport/ID={\"ID\"}"
              ] .
