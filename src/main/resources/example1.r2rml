// Database settings
R2RConn("jdbc:mysql://localhost/wacko","wacko","wacko");

// Base Namespace Definition
R2RBase("http://aksw.org/triplify/");

// Schema Base Namespace Definition
R2RSchemaBase("http://triplify.org/vocabulary/blog/");

// Some Prefix Definitions
R2RNS("foaf", "http://xmlns.com/foaf/0.1/");
R2RNS("sioc", "http://rdfs.org/sioc/ns#");
R2RNS("dcterms", "http://purl.org/dc/terms/");

// Mappings
R2RMap("wakka_users",    "user",    "foaf:Person",    "name",  "foaf:name", "NULL", "foaf:mbox"); 
R2RMap("wakka_pages",   "page",   "sioc:Post",   "tag", "NULL", "dcterms:title", "NULL", "NULL","sioc:content","NULL", "NULL", "sioc:has_creator->user", "sioc:has_modifier->user");